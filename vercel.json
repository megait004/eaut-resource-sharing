{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "buildCommand": "",
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/api/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}